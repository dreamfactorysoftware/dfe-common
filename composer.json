{
    "name":              "dreamfactory/dfe-common",
    "description":       "DreamFactory(tm) Enterprise Common Component Package",
    "homepage":          "https://www.dreamfactory.com/",
    "keywords":          [
        "dreamfactory",
        "fabric",
        "utility",
        "queue",
        "common"
    ],
    "license":           [
        "proprietary"
    ],
    "type":              "library",
    "authors":           [
        {
            "name":  "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        }
    ],
    "repositories":      [
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-database.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-storage.git"
        }
    ],
    "prefer-stable":     false,
    "minimum-stability": "dev",
    "require":           {
        "dreamfactory/php-utils":      "1.1.*",
        "wpb/string-blade-compiler":   "2.*@dev",
        "ruflin/elastica":             "*",
        "league/flysystem-ziparchive": "*"
    },
    "require-dev":       {
        "dreamfactory/dfe-storage":    "dev-feature/l52-upgrade as master",
        "dreamfactory/dfe-database":   "dev-feature/l52-upgrade as master",
        "laravel/framework":    "5.2.*",
        "symfony/dom-crawler":  "~3.0",
        "symfony/css-selector": "~3.0",
        "phpunit/phpunit":      "~4.0",
        "way/generators":       "~3.0",
        "phpspec/phpspec":      "~2.1"
    },
    "autoload":          {
        "psr-4": {
            "DreamFactory\\Enterprise\\Common\\": "src/"
        }
    },
    "autoload-dev":      {
        "classmap": [
            "tests/TestCase.php",
            "tests/CommandTestCase.php"
        ]
    },
    "config":            {
        "preferred-install": "dist"
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
