{
	"name":              "dreamfactory/dfe-common",
	"description":       "DreamFactory Enterprise(tm) Common Components",
	"homepage":          "https://www.dreamfactory.com/",
	"keywords":          [
		"dreamfactory",
		"fabric",
		"utility",
		"queue",
		"common"
	],
	"license":           ["proprietary"],
	"type":              "library",
	"authors":           [
		{
			"name":  "Jerry Ablan",
			"email": "jerryablan@dreamfactory.com"
		}
	],
	"repositories":      [
		{
			"type": "vcs",
			"url":  "git@github.com:dreamfactorysoftware/dfe-common.git"
		},
		{
			"type": "vcs",
			"url":  "git@github.com:dreamfactorysoftware/dfe-database.git"
		}
	],
	"minimum-stability": "dev",
	"require":           {
		"dreamfactory/php-utils":      "dev-develop as dev-master",
		"wpb/string-blade-compiler":   "2.*@dev",
		"league/flysystem-sftp":       "*",
		"league/flysystem-ziparchive": "*"
	},
	"require-dev":       {
		"dreamfactory/dfe-database": "dev-develop as dev-master",
		"dreamfactory/dfe-common":   "dev-develop as dev-master",
		"laravel/framework":         "5.0.*",
		"phpunit/phpunit":           "~4.0",
		"phpspec/phpspec":           "~2.1"
	},
	"extra":             {
		"branch-alias": {
			"dev-develop": "0.1.x-dev"
		}
	},
	"autoload":          {
		"psr-4": {
			"DreamFactory\\Enterprise\\Common\\": "src/"
		}
	},
	"config":            {
		"preferred-install": "dist"
	}
}
